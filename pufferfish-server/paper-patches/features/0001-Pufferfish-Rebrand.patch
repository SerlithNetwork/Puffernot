+    // Pufferfish start - Rebrand
+    private static int fetchDistanceFromWorkflows(final String repo, final int workflowsBuild) {
+        try {
+            final HttpURLConnection connection = (HttpURLConnection) URI.create("https://api.github.com/repos/%s/actions/workflows/build-1-21-6.yml/runs?per_page=15".formatted(repo)).toURL().openConnection();
+            connection.connect();
+            if (connection.getResponseCode() == HttpURLConnection.HTTP_NOT_FOUND) return DISTANCE_UNKNOWN; // Unknown commit
+            try (final BufferedReader reader = new BufferedReader(new InputStreamReader(connection.getInputStream(), StandardCharsets.UTF_8))) {
+                final JsonObject obj = new Gson().fromJson(reader, JsonObject.class);
+                final JsonArray runs = obj.getAsJsonArray("workflow_runs");
+                final Optional<JsonElement> run = runs.asList().stream().filter(r -> r.getAsJsonObject().get("conclusion").getAsString().equalsIgnoreCase("success")).findFirst();
+                return run.map(jsonElement -> jsonElement.getAsJsonObject().get("run_number").getAsInt() - workflowsBuild).orElse(DISTANCE_ERROR);
+            } catch (final JsonSyntaxException | NumberFormatException e) {
+                LOGGER.error("Error parsing json from GitHub's API", e);
+                return DISTANCE_ERROR;
+            }
+        } catch (IOException e) {
+            LOGGER.error("Error while parsing version", e);
+            return DISTANCE_ERROR;
+        }
+    }
+    // Pufferfish end - Rebrand
